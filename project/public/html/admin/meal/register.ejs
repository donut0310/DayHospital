<%- include('../header') %>

<form action="/admin/meal" method="post" enctype="multipart/form-data">
  <div class="form-group">
    <label for="exampleInputPassword1">파일명</label>
    <input type="file" class="form-control" name="file_name" id="file_name" accept="image/png, image/jpeg, image/jpg"
      multiple required>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">제목</label>
    <input type="text" class="form-control" name="title" id="title">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">내용</label>
    <textarea type="text" class="form-control" name="content" id="content" cols="30" rows="10" required></textarea>
  </div>

  <button class="btn btn-default" type="submit">등록</button>
</form>


<%- include('../footer') %>